.container{
  padding-bottom: 16vh;
}
.bottom{
  position: fixed;width: 750rpx;color:#ededed ;background: #4c87e5;text-align: center;left: 0px;bottom: 0px;height:8vh;
  line-height:8vh;
}
.data_item{margin-top: 0rpx;border-top: 1px solid white;}
.project_name{
  background: rgb(213, 233, 255);
  padding: 20rpx 0 20rpx 20rpx;
}
.project_name text{
  font-weight: bold;
  border-left: 8rpx solid #4c86e5;
  padding-left: 20rpx;
}
.project_item{
  width: 100%;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  flex-direction: row;
  position: relative;
  margin-left: 20rpx;
  padding: 30rpx 0;
}
.project_item:last-child{
  border-bottom: 0px;
}

.exam_times{
  flex:1;
  text-align: right;
  margin-right: 40rpx;
}